		<TextField fx:id="inputTextField" promptText="Eingabe" />
		<Button text="Weiter" onAction="#goToOutput" />
	</children>
	<padding>
		<Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
	</padding>
</VBox>
